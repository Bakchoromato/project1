#include <iostream>

using namespace std;

int main()
{
    int x, y;
    cout << "x: ";
    cin >> x;
    cout << "y: ";
    cin >> y;
    
    if((x >= -2 && x <= 2) && (y >= -2 && y <= 2))
        cout << "Target is hit!" << endl;
    else
        cout << "You missed!" << endl;
        
    return 0;
}
