#include <iostream>

using namespace std;

int main()
{
    int grade;
    cout << "Мама: какую какую оценку ты получил ?" << endl;
    cout << "(2-) " << endl;
    cin >> grade;
    if(grade == 5){
        cout << "Мама: молодэс я купить тэбэ щокаладка" << endl;
    } else if(grade == 8){
        cout << "Мама:молодец но ты можешь лучше" << endl;
    } else
    {
        cout << "Мама:молодец но ты можешь лучше" << endl;
    }

    return 0;    
}
